package com.cursojava04.modulo4.arreglos;

public class Ej1 {
	
	
	 public static void main(String[] args) {
	    // Declaraci칩n e inicializaci칩n de un arreglo de enteros
	    int[] numeros = {1, 2, 3, 4, 5};

	    // Accediendo a los elementos del arreglo y mostr치ndolos
	    for (int i = 0; i < numeros.length; i++) {
	    System.out.println("Elemento en la posici칩n " + i + ": " + numeros[i]);
	    }
  }
	


}
