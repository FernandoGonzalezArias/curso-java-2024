package com.cursojava04.modulo4.banco;

public class Main {

	public static void main(String[] args) {
		//CREAMOS EL OBJETO DE LA CLASE
		CuentaBancaria cuenta = new CuentaBancaria();
		//LLAMAMOS O INSTANCIAMOS EL OBJETO
		cuenta.generarSaludoApertura();
		
		// INGRESAMOS LOS VALORES A LOS ATRIBUTOS 
		cuenta.saldoActual = 123.321;
		cuenta.numeroDeCuenta = 123;
		cuenta.titular = "Fernando";
		
		System.out.println(cuenta.titular();

	}

}
